{
  "name": "events-manager",
  "version": "1.0.0",
  "description": "An event-manager app,andela developer challenge",
  "main": "app.js",
  "engines": {
    "node": "8.11.1",
    "npm": "6.1.0"
  },
  "scripts": {
    "localtest": "export NODE_ENV=test&&nyc --reporter=html --reporter=text --reporter=lcov mocha ./server/test/tests.js --compilers js:babel-core/register --timeout 25000 --exit",
    "test": "nyc --reporter=html --reporter=text --reporter=lcov mocha --timeout 250000 ./server/test/tests.js --compilers js:babel-core/register --exit",
    "start:dev": "nodemon ./server/app --exec babel-node ./server/app",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel ./server/ -d ./dist ./client/ -d ./dist",
    "start": "npm run build && node ./dist/app.js",
    "start:react": "nodemon ./client/server.js --exec babel-node ./client/server.js",
    "sendmail": "babel-node ./server/helpers/mailer.js",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.prod.js",
    "jest": "jest client/tests/ --watch --coverage --collectCoverageFrom=client/**/*.{js,jsx} --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Efosaok/Events-Manager.git"
